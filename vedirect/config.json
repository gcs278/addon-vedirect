{
  "name": "vedirect",
  "version": "1.0.1",
  "slug": "vedirect",
  "description": "Victron vedirect Mqtt Service",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "system",
  "boot": "auto",
  "host_network": true,
  "apparmor": false,
  "host_dbus": true,
  "full_access": true,
  "privileged": [
    "NET_ADMIN",
    "SYS_ADMIN",
    "SYS_RAWIO",
    "SYS_TIME",
    "SYS_NICE"
  ],
  "devices": [
    "/dev/mem:/dev/mem:rwm"
  ],
  "map": [
    "config:rw",
    "ssl:rw",
    "addons:rw",
    "share:rw",
    "backup:rw"
  ],
  "timeout": 20,
  "options": {
    "mqtt_topicprefix": "vedirect",
    "mqtt_user": null,
    "mqtt_password": null
  },
  "schema": {
    "mqtt_topicprefix": "str",
    "mqtt_user": "str",
    "mqtt_password": "str"
  }
} 
